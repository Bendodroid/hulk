{
    "name": "motionPlanner",
    "enabled": true,
    "config": {
                "center_x": 0.0,
                "center_y": 0.0,
                "transformation": {
                    "key": "Brain.RobotPosition",
                    "key_lambda": "output = input['pose']"
                },
                "targetPosition": {
                    "key": "Brain.ActionCommand",
                    "key_lambda": "output = input['body']['walkTarget'][0]",
                    "targetCircleDiameter": 0.35,
                    "targetColor": "#ffffff"
                },
                "displacementVector": {
                    "key": "Motion.MotionPlanner.displacementVector",
                    "key_lambda": "output = input",
                    "lineWidth": 0.05,
                    "lineColor": "#ffffff"
                }
            }
}
